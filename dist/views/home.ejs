<%- include("partials/header") %>
    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>


    <% posts.forEach(post=> { %>
        <h1>
            <%= post.title %>
        </h1>
        <p>
            <%= truncate(post.text, { length: 100, omission: '...' })%>
                <a href="/posts/<%= post._id.toString() %> "> Read more</a>
        </p>

        <% }) %>
            <%- include("partials/footer") %>